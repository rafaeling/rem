<script type="text/javascript">

var menuTimeOut;
//var finDeCarga=0;
var finDeCarga;
var menuActual = "";
var url_raiz = "http://www.pccomponentes.com/";


$(document).ready(function() {
	$(document).one('mousemove', function() {  
		  $('.cargaSubMenu').mouseover(function() {
			clearTimeout(menuTimeOut);
			var id_elemento = $(this).attr("id");
			var elementoCarga = id_elemento.replace("btn_menu_","");
			//$("#prueba_id").val(elementoCarga);
			mostrarImagenMenuSeleccionado(elementoCarga);
			if(elementoCarga!="home")
			{
				menuActual = elementoCarga;
				menuTimeOut = setTimeout(function(){cargarSubmenu(elementoCarga)},500);
			}
			else
			{
				ocultarSubmenu();
			}
			
		});
		
		$('.menu-pccomponentes').mouseleave(function() {
							ocultarSubmenu();
						});
	});
});

function mostrarImagenMenuSeleccionado(elemento)
{
	ocultarImagenMenus(elemento);
	$("#btn_menu_"+elemento).css("background","url("+url_raiz+"nuevo_menu/img/mnu-pico.gif) bottom center no-repeat");
}

function ocultarImagenMenus(elemento)
{
	$(".cargaSubMenu").css("background","none");
}

function ocultarSubmenu(){
		finDeCarga=1;
		clearTimeout(menuTimeOut);
		ocultarTodosSubmenus();
		ocultarImagenMenus();
}

function ocultarTodosSubmenus(){
		$("#submenu_cargando").css("display","none");
		$("._submenu_cargado").css("display","none");
		//$(".cargaSubMenu").css("background","url(img/mnu-pico.gif) bottom center no-repeat");
		//$(".cargaSubMenu").css("background","none");
}


function cargarSubmenu(elemento){
	
		finDeCarga=0;
		ocultarTodosSubmenus();
		mostrarImagenMenuSeleccionado(elemento);
			
		if(finDeCarga==0)
			$("#submenu_cargando").css("display","block");
		else
			ocultarSubmenu();
		
		if($("#submenu_"+elemento).html()!="")
		{
			$("#submenu_cargando").css("display","none");
			$("#submenu_"+elemento).css("display","block");
		}
		else
		{
			
			if(finDeCarga==0)
			{
					$.ajax({
				  url: url_raiz+"nuevo_menu/inc_menu_dinamico.php",
				  async:true,
				  beforeSend: function(objeto){
						if(finDeCarga==1)
						{
							ocultarSubmenu();
							objeto.abort();
						}
					},
				  type: "get",
				  data: "opcion="+elemento,
				  success: function(data){
				
					if(finDeCarga==0)
					{
						if(menuActual==elemento)
						{
						
							var imageCount = $(data).find('img').length;
							  var imagesLoaded = 0;
							  $("#submenu_"+elemento).empty();
							  $(data).appendTo('#submenu_'+elemento)
									 .find('img')
									 .live('load', function() {
										 ++imagesLoaded;
										 if (imagesLoaded >= imageCount) {
											$("#submenu_cargando").css("display","none");
											$("#submenu_"+elemento).css("display","block");
										 }
									  });
							
								setTimeout( function() {
										if(finDeCarga==0)
										{
											$("#submenu_cargando").css("display","none");
											$("#submenu_"+elemento).css("display","block");
										}
									
									}, 500 );
							/*		  
							$("#submenu_"+elemento).empty().append(data);
							$("#submenu_cargando").css("display","none");
							$("#submenu_"+elemento).css("display","block");
							*/
						}
						
					}
					else
					{
						ocultarSubmenu();
					}
				  },
				  error:function(){
					ocultarSubmenu();
				  }   
				}); 
			}
			else
			{
				ocultarSubmenu();
			}
		}
}



function mostrarSubmenuSubfamilia(id_elemento,elementOver)
{	
	if(id_elemento!="")
	{
		ocultarSubmenusSubfamiliasTodos();
		
		if($("#"+id_elemento).css("display")=="none")
		{
			var variableTop = $(elementOver).position().top-23;
			var variableLeft = $(elementOver).width()+30;

			$("#"+id_elemento).css("top",variableTop);
			$("#"+id_elemento).css("margin-left",variableLeft);
			$("#"+id_elemento).css("display","block");
			
			$(".elementoMenusubfamilia").mouseleave(function () {
							$("#"+id_elemento).css("display","none");
					});
			
		}
	}
}


function ocultarSubmenusSubfamiliasTodos()
{
	$(".claseSubfamiliaSubmenu").css("display","none");
}





</script>
